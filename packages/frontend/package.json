{
  "name": "@onder/frontend",
  "version": "0.0.1",
  "description": "Frontend Part for Onder Nodes",
  "repository": "https://github.com/onderPlatform/onder",
  "license": "AGPL-3.0",
  "scripts": {
    "build": "exit 0",
    "build-hardware": "cross-env BUILD_ENV=hardware node build/build.js",
    "build-consumer": "cross-env BUILD_ENV=consumer node build/build.js",
    "build-operator": "cross-env BUILD_ENV=operator node build/build.js",
    "generate": "yarn generate_error_serializer",
    "generate_error_serializer": "xsltproc -o src/common/error_serializer.ts resources/error_serializer.xsl ../interfaces/resources/errors.xml",
    "generate_web_serializer": "xsltproc -o src/common/web_serializer.ts resources/web_serializer.xsl ../interfaces/resources/web.xml",
    "dev-server": "yarn generate && cross-env BUILD_ENV=$BUILD_ENV webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --host 0.0.0.0",
    "dev-hardware": "BUILD_ENV=hardware npm run dev-server",
    "dev-consumer": "BUILD_ENV=consumer npm run dev-server",
    "dev-operator": "BUILD_ENV=operator npm run dev-server",
    "lint": "npm run tslint",
    "tslint": "tslint -t stylish -p '.' -e './node_modules/**/*.ts'",
    "eslint": "eslint --ext .js,.vue src test/unit"
  },
